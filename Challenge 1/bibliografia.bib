  isbn            = {978-1-5386-9534-0},
  issn            = {2469-5556},
  journal         = {2019 5th International Conference on Advanced Computing & Communication Systems (ICACCS)},
  keywords        = {Phishing, Uniform resource locators, Google, Tools, Feature extraction, Blacklisting, Phishing websites, Machine Learning, anti-phishing, phishing attack, security and privacy, website features, classification, phishing approaches},
}

@InProceedings{Aravindhan2016,
